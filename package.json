{
  "name": "riedbergtv",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@sentry/browser": "^5.0.8",
    "body-parser": "^1.18.3",
    "bootstrap": "^4.2.1",
    "concurrently": "^4.1.0",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "moment": "^2.29.2",
    "node-sass": "^4.11.0",
    "node-sass-chokidar": "^1.3.4",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-markdown": "^4.0.3",
    "react-router-dom": "^4.3.1",
    "react-scripts-ts": "3.1.0",
    "react-slick": "^0.23.1",
    "react-toastify": "^4.4.0",
    "react-video-play": "^2.0.3",
    "rxjs": "^6.4.0",
    "slick-carousel": "^1.8.1",
    "video-react": "^0.13.3"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/ --output-style compressed",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start": "npx concurrently -n react,scss -c blue,green \"react-scripts-ts start\" \"npm:watch-css\"",
    "build": "npm run build-css && react-scripts-ts build && cp _redirects build/",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "start-server": "tsc -p tsconfig.server.json && node build-server/index.js"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.7.1",
    "@types/body-parser": "^1.17.0",
    "@types/dotenv": "^4.0.3",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.5",
    "@types/moment": "^2.13.0",
    "@types/node": "^10.11.7",
    "@types/react": "^16.4.16",
    "@types/react-dom": "^16.0.9",
    "@types/react-router-dom": "^4.3.1",
    "@types/react-slick": "^0.23.2",
    "@types/react-toastify": "^4.0.1",
    "nodemon": "^1.18.4",
    "typescript": "^3.1.3"
  }
}
